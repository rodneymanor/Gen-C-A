{
  "name": "gen-c-alpha-dashboard",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "concurrently -k \"npm run backend:dev\" \"vite\"",
    "dev:old": "concurrently \"npm run api\" \"vite\"",
    "dev:frontend": "vite",
    "api": "node server.js",
    "backend:dev": "tsx apps/backend/src/server.ts",
    "build": "vite build",
    "preview": "vite preview",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives",
    "type-check": "tsc --noEmit",
    "test": "vitest",
    "test:run": "vitest run",
    "test:coverage": "vitest run --coverage",
    "test:ui": "vitest --ui",
    "test:e2e:collections": "node test-collections-modal.js",
    "test:services": "ts-node __tests__/TestRunner.ts",
    "test:services:all": "ts-node __tests__/TestRunner.ts all",
    "test:services:unit": "ts-node __tests__/TestRunner.ts unit",
    "test:services:integration": "ts-node __tests__/TestRunner.ts integration",
    "test:services:performance": "ts-node __tests__/TestRunner.ts performance",
    "test:services:validation": "ts-node __tests__/TestRunner.ts validation",
    "test:services:watch": "ts-node __tests__/TestRunner.ts watch",
    "test:services:ui": "ts-node __tests__/TestRunner.ts ui",
    "test:unit": "vitest run __tests__/unit/**/*.test.ts",
    "test:unit:auth": "vitest run __tests__/unit/services/auth/**/*.test.ts",
    "test:unit:collections": "vitest run __tests__/unit/lib/CollectionsService.test.ts",
    "test:unit:video": "vitest run __tests__/unit/services/VideoProcessingService.test.ts",
    "test:integration": "vitest run __tests__/integration/**/*.test.ts",
    "test:validation": "vitest run __tests__/validation/**/*.test.ts",
    "test:performance": "vitest run __tests__/performance/**/*.test.ts --testTimeout=120000",
    "test:coverage:services": "vitest run --coverage --coverage.include=\"src/services/**/*.ts\" --coverage.include=\"src/lib/collections-service.ts\"",
    "test:ci": "vitest run --coverage --reporter=json --outputFile=__tests__/reports/test-results.json",
    "quality-check": "npm run type-check && npm run lint && npm run test:coverage",
    "sub-agent": "node scripts/sub-agent.cjs",
    "sub-agent:init": "npm run sub-agent init",
    "sub-agent:analyze": "npm run sub-agent analyze",
    "sub-agent:implement": "npm run sub-agent implement",
    "sub-agent:watch": "npm run sub-agent watch",
    "sub-agent:validate": "npm run sub-agent validate",
    "sub-agent:status": "npm run sub-agent status",
    "migrate:brand-voices": "node ./scripts/migrate-brand-voices.js",
    "format": "prettier \"**/*.{js,jsx,ts,tsx,css,md,json}\" --write",
    "format:check": "prettier \"**/*.{js,jsx,ts,tsx,css,md,json}\" --check",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build"
  },
  "dependencies": {
    "@atlaskit/avatar": "^25.1.19",
    "@atlaskit/badge": "^18.1.6",
    "@atlaskit/button": "^23.4.8",
    "@atlaskit/css-reset": "^7.3.6",
    "@atlaskit/dropdown-menu": "^16.3.11",
    "@atlaskit/dynamic-table": "^18.2.15",
    "@atlaskit/form": "^12.6.1",
    "@atlaskit/icon": "^28.1.2",
    "@atlaskit/icon-object": "^7.2.2",
    "@atlaskit/lozenge": "^13.0.11",
    "@atlaskit/modal-dialog": "^14.3.13",
    "@atlaskit/page-header": "^12.1.5",
    "@atlaskit/primitives": "^14.14.2",
    "@atlaskit/section-message": "^8.7.4",
    "@atlaskit/select": "^21.2.10",
    "@atlaskit/tabs": "^18.1.10",
    "@atlaskit/textarea": "^8.0.9",
    "@atlaskit/textfield": "^8.0.11",
    "@atlaskit/toggle": "^15.1.7",
    "@atlaskit/tokens": "^6.3.1",
    "@emotion/react": "^11.11.1",
    "@emotion/styled": "^11.11.0",
    "@google/generative-ai": "^0.24.1",
    "chokidar": "^4.0.3",
    "clsx": "^2.0.0",
    "commander": "^14.0.1",
    "concurrently": "^8.2.2",
    "cors": "^2.8.5",
    "date-fns": "^2.30.0",
    "dotenv": "^17.2.2",
    "express": "^4.21.2",
    "fast-glob": "^3.3.3",
    "firebase": "^12.2.1",
    "firebase-admin": "^13.5.0",
    "framer-motion": "^10.16.0",
    "glob": "^11.0.3",
    "lucide-react": "^0.263.1",
    "mammoth": "^1.10.0",
    "markdown-it": "^14.1.0",
    "natural": "^8.1.0",
    "openai": "^5.20.2",
    "pdf-parse": "^1.1.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.15.0",
    "vite-express": "^0.21.1"
  },
  "devDependencies": {
    "@atlaskit/eslint-plugin-design-system": "^13.20.1",
    "@chromatic-com/storybook": "^4.1.1",
    "@storybook/addon-a11y": "^9.1.6",
    "@storybook/addon-docs": "^9.1.6",
    "@storybook/addon-onboarding": "^9.1.6",
    "@storybook/addon-vitest": "^9.1.6",
    "@storybook/react-vite": "^9.1.6",
    "@testing-library/jest-dom": "^6.8.0",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/node": "^24.3.1",
    "@types/puppeteer": "^5.4.7",
    "@types/react": "^18.2.15",
    "@types/react-dom": "^18.2.7",
    "@typescript-eslint/eslint-plugin": "^8.12.0",
    "@typescript-eslint/parser": "^8.12.0",
    "@vitejs/plugin-react": "^4.0.3",
    "eslint": "^8.57.1",
    "eslint-config-prettier": "^10.1.8",
    "eslint-import-resolver-typescript": "^3.6.1",
    "eslint-plugin-import": "^2.32.0",
    "eslint-plugin-jsx-a11y": "^6.10.2",
    "eslint-plugin-prettier": "^5.5.4",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.3",
    "eslint-plugin-storybook": "^9.1.6",
    "jsdom": "^26.1.0",
    "prettier": "^3.6.2",
    "puppeteer": "^24.20.0",
    "storybook": "^9.1.6",
    "ts-node": "^10.9.2",
    "tsx": "^4.20.5",
    "typescript": "^5.0.2",
    "vite": "^5.4.20",
    "vitest": "^3.2.4",
    "@vitest/browser": "3.2.4",
    "playwright": "^1.55.0",
    "@vitest/coverage-v8": "3.2.4"
  }
}
