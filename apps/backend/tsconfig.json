{
  "extends": "../../tsconfig.json",
  "compilerOptions": {
    "rootDir": "../..",
    "outDir": "../../dist/backend",
    "moduleResolution": "node",
    "module": "ESNext",
    "noEmit": false,
    "allowJs": true,
    "resolveJsonModule": true,
    "types": ["node"],
    "baseUrl": ".",
    "esModuleInterop": true,
    "allowSyntheticDefaultImports": true,
    "skipLibCheck": true
  },
  "include": ["src", "../../src/types/**/*.d.ts"]
}
